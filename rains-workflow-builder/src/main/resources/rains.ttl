@prefix : <https://w3id.org/rains#> .
@prefix mls: <http://www.w3.org/ns/mls#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sao: <https://w3id.org/sao#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix prov: <http://www.w3.org/ns/prov-o#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rains: <https:w3id.org/rains#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix ep-plan: <https://w3id.org/ep-plan#> .
@base <https://w3id.org/rains> .

<https://w3id.org/rains> rdf:type owl:Ontology ;
                          dcterms:creator <https://www.abdn.ac.uk/ncs/profiles/iman.naja> ,
                                          <https://www.abdn.ac.uk/ncs/profiles/milan.markovic/> ,
                                          "https://www.abdn.ac.uk/ncs/profiles/p.edwards" ;
                          dcterms:license <https://creativecommons.org/licenses/by-nc-sa/4.0/> ;
                          dcterms:title "The RAInS Ontology"@en ;
                          rdfs:comment "To contact the RAInS team, please send an email to therainsproject [at] gmail [dot] com"@en ;
                          rdfs:label "The RAInS Ontology"@en ;
                          owl:versionInfo "2.0, March 2021"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/creator
dcterms:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/license
dcterms:license rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/title
dcterms:title rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://purl.org/dc/terms/isReferencedBy
dcterms:isReferencedBy rdf:type owl:ObjectProperty .


###  http://purl.org/dc/terms/references
dcterms:references rdf:type owl:ObjectProperty .


###  http://purl.org/dc/terms/replaces
dcterms:replaces rdf:type owl:ObjectProperty .


###  https://w3id.org/ep-plan#hasInputVariable
ep-plan:hasInputVariable rdf:type owl:ObjectProperty .


###  https://w3id.org/ep-plan#hasOutputVariable
ep-plan:hasOutputVariable rdf:type owl:ObjectProperty .


###  https://w3id.org/rains#hasEvaluationResult
:hasEvaluationResult rdf:type owl:ObjectProperty ;
                     owl:inverseOf :isEvaluationResultOf ;
                     rdfs:domain :EvaluationMeassure ;
                     rdfs:range :EvaluationResult .


###  https://w3id.org/rains#hasRealizableObjectCharacteristic
:hasRealizableObjectCharacteristic rdf:type owl:ObjectProperty ;
                                   owl:inverseOf :isCharacteristicOfRealizableObject ;
                                   rdfs:domain :RealizableObject ;
                                   rdfs:range :RealizableObjectCharacteristic ;
                                   rdfs:comment "A relation that links a realizable object (e.g., model) to the information element describing its characteristic." ;
                                   rdfs:label "hasRealizableObjectCharacteristic" .


###  https://w3id.org/rains#isCharacteristicOfRealizableObject
:isCharacteristicOfRealizableObject rdf:type owl:ObjectProperty ;
                                    rdfs:domain :RealizableObjectCharacteristic ;
                                    rdfs:range :RealizableObject ;
                                    rdfs:comment "Inverse of hasRealizableObjectCharacteristic" ;
                                    rdfs:label "isCharacteristicOfRealizableObject" .


###  https://w3id.org/rains#isEvaluationResultOf
:isEvaluationResultOf rdf:type owl:ObjectProperty .


#################################################################
#    Data properties
#################################################################

###  https://w3id.org/rains#computedOnDecisionThreshold
:computedOnDecisionThreshold rdf:type owl:DatatypeProperty ;
                             rdfs:domain :EvaluationResult .


###  https://w3id.org/rains#computedOnSlice
:computedOnSlice rdf:type owl:DatatypeProperty ;
                 rdfs:domain :EvaluationMeassure .


###  https://w3id.org/rains#hasBase64Image
:hasBase64Image rdf:type owl:DatatypeProperty ;
                rdfs:domain sao:InformationElement ;
                rdfs:range xsd:string ;
                rdfs:comment "A value indicating what an :InformationElement's base 64 image is in String format." ;
                rdfs:label "hasBase64Image" .


###  https://w3id.org/rains#hasMitigationStrategy
:hasMitigationStrategy rdf:type owl:DatatypeProperty ;
                       rdfs:domain :Risk ;
                       rdfs:range xsd:string ;
                       rdfs:comment "A value indicating what a :Risk's mitigation strategy is, expressed in String format." ;
                       rdfs:label "hasMitigationStrategy" .


###  https://w3id.org/rains#hasResultLowerBound
:hasResultLowerBound rdf:type owl:DatatypeProperty ;
                     rdfs:domain :EvaluationResult .


###  https://w3id.org/rains#hasResultUpperBound
:hasResultUpperBound rdf:type owl:DatatypeProperty ;
                     rdfs:domain :EvaluationResult .


###  https://w3id.org/rains#hasResultValue
:hasResultValue rdf:type owl:DatatypeProperty ;
                rdfs:domain :EvaluationResult .


###  https://w3id.org/rains#isReusedObject
:isReusedObject rdf:type owl:DatatypeProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain :RealizableObject ;
                rdfs:range xsd:boolean ;
                rdfs:comment "A value indicating whether the :RealizableObject is a third party resource." ;
                rdfs:label "isReusedObject" .


###  https://w3id.org/rains#version
:version rdf:type owl:DatatypeProperty ;
         rdfs:domain :RealizableObject ;
         rdfs:range xsd:positiveInteger ;
         rdfs:comment "A value indicating what a :RealizableObject's version is." ;
         rdfs:label "version" .


###  https://w3id.org/rains#versionDate
:versionDate rdf:type owl:DatatypeProperty ;
             rdfs:domain :RealizableObject ;
             rdfs:range xsd:dateTime ;
             rdfs:comment "A value indicating what the version date of a :RealizableObject's is." ;
             rdfs:label "versionDate" .


###  https://w3id.org/rains#versionNote
:versionNote rdf:type owl:DatatypeProperty ;
             rdfs:domain :RealizableObject ;
             rdfs:range xsd:string ;
             rdfs:comment "A value describing a note about the version date of a :RealizableObject." ;
             rdfs:label "versionNote" .


#################################################################
#    Classes
#################################################################

###  http://ontosoft.org/software#Software
<http://ontosoft.org/software#Software> rdf:type owl:Class ;
                                        rdfs:subClassOf <http://www.w3.org/ns/prov#SoftwareAgent> ,
                                                        ep-plan:Agent ,
                                                        sao:InformationElement .


###  http://purl.org/dc/terms/LicenseDocument
dcterms:LicenseDocument rdf:type owl:Class ;
                        rdfs:subClassOf sao:InformationElement .


###  http://www.w3.org/ns/mls#Algorithm
mls:Algorithm rdf:type owl:Class ;
              rdfs:subClassOf :RealizableObjectCharacteristic .


###  http://www.w3.org/ns/mls#Dataset
mls:Dataset rdf:type owl:Class ;
            rdfs:subClassOf :RealizableObject ,
                            [ rdf:type owl:Class ;
                              owl:unionOf ( :ExcludedObject
                                            :ImplementedObject
                                          )
                            ] .


###  http://www.w3.org/ns/mls#DatasetCharacteristic
mls:DatasetCharacteristic rdf:type owl:Class ;
                          rdfs:subClassOf :RealizableObjectCharacteristic .


###  http://www.w3.org/ns/mls#EvaluationProcedure
mls:EvaluationProcedure rdf:type owl:Class ;
                        rdfs:subClassOf :EvaluationProcedure .


###  http://www.w3.org/ns/mls#HyperParameter
mls:HyperParameter rdf:type owl:Class ;
                   rdfs:subClassOf :Parameter .


###  http://www.w3.org/ns/mls#Model
mls:Model rdf:type owl:Class ;
          rdfs:subClassOf :RealizableObject ,
                          [ rdf:type owl:Class ;
                            owl:unionOf ( :ExcludedObject
                                          :ImplementedObject
                                        )
                          ] .


###  http://www.w3.org/ns/mls#ModelCharacteristic
mls:ModelCharacteristic rdf:type owl:Class ;
                        rdfs:subClassOf :RealizableObjectCharacteristic .


###  http://www.w3.org/ns/mls#ModelEvaluation
mls:ModelEvaluation rdf:type owl:Class ;
                    rdfs:subClassOf :EvaluationResult .


###  http://www.w3.org/ns/prov#Agent
<http://www.w3.org/ns/prov#Agent> rdf:type owl:Class .


###  http://www.w3.org/ns/prov#SoftwareAgent
<http://www.w3.org/ns/prov#SoftwareAgent> rdf:type owl:Class ;
                                          rdfs:subClassOf <http://www.w3.org/ns/prov#Agent> .


###  https://w3id.org/ep-plan#Agent
ep-plan:Agent rdf:type owl:Class ;
              rdfs:subClassOf <http://www.w3.org/ns/prov#Agent> .


###  https://w3id.org/rains#AI_System
:AI_System rdf:type owl:Class ;
           rdfs:subClassOf sao:System ;
           rdfs:comment "The AI System for which the accountability plan is specified." ;
           rdfs:label "AI System" .


###  https://w3id.org/rains#ApprovalDecision
:ApprovalDecision rdf:type owl:Class ;
                  rdfs:subClassOf :HumanDecision ;
                  rdfs:comment "A specific type of a planned human decision representing approvals and sign offs." ;
                  rdfs:label "Approval Decision" .


###  https://w3id.org/rains#Bias
:Bias rdf:type owl:Class ;
      rdfs:subClassOf :Risk ;
      rdfs:comment "A type of :Risk  which records a specific piece of  information detailing a potential inclination or prejudice related to some aspects of :AI_Sytem (e.g., a training dataset containing more data about males when both genders will be considered by the ML model)" ;
      rdfs:label "Bias" .


###  https://w3id.org/rains#Certificate
:Certificate rdf:type owl:Class ;
             rdfs:subClassOf sao:AccountableResult ,
                             sao:InformationElement ;
             rdfs:comment "A specific type of planned sao:AccountableResult which represents a high reference to a certificate  (e.g. an ISO certificate) that is expected to be granted during the implementation, deployment, or use and maintenance life cycle stages." ;
             rdfs:label "Certificate" .


###  https://w3id.org/rains#CertificationDecision
:CertificationDecision rdf:type owl:Class ;
                       rdfs:subClassOf :ComplianceAssuranceDecision ;
                       rdfs:comment "A specific type of a planned human compliance assurance decision representing certification granting." ;
                       rdfs:label "Certification Decision" .


###  https://w3id.org/rains#ChangeJustification
:ChangeJustification rdf:type owl:Class ;
                     rdfs:subClassOf sao:InformationElement ;
                     rdfs:label "Change Justification" .


###  https://w3id.org/rains#ChangeLog
:ChangeLog rdf:type owl:Class ;
           rdfs:subClassOf sao:AccountableResult ;
           rdfs:comment "A specific type of planned sao:AccountableResult that is expected to be produced during the implementation or deployment stages in order to describe any change from a design specification or guideline ." ;
           rdfs:label "Change Log" .


###  https://w3id.org/rains#ComplianceAssuranceDecision
:ComplianceAssuranceDecision rdf:type owl:Class ;
                             rdfs:subClassOf :HumanDecision ;
                             rdfs:comment "A specific type of a planned human decision representing confirmation of compliance of some sao:AccountableResult (e.g., a model specification) with its compliance requirements." ;
                             rdfs:label "Compliance Assurance Decision" .


###  https://w3id.org/rains#ComplianceEntity
:ComplianceEntity rdf:type owl:Class ;
                  rdfs:subClassOf sao:InformationElement ;
                  rdfs:comment "A specific type of sao:InformationElement which records a specific piece of  information about  a compliance requirement that is to be complied with as part of an information realization collection." ;
                  rdfs:label "Compliance Entity" .


###  https://w3id.org/rains#ComplianceSpecification
:ComplianceSpecification rdf:type owl:Class ;
                         rdfs:subClassOf :DesignSpecification ;
                         rdfs:comment "A specific type of design specification  which represents the  description of a compliance requirement." ;
                         rdfs:label "Compliance Specification" .


###  https://w3id.org/rains#DataCollectionProcedure
:DataCollectionProcedure rdf:type owl:Class ;
                         rdfs:subClassOf sao:InformationElement ;
                         rdfs:comment "A specific type of sao:InformationElement which records a specific piece of  information detailing how the data is collected. This includes the information about mechanism used for collecting data (e.g. hardware sensors, software program, or manually), the timeframe over which the data was collected, any consent reporting, etc." ;
                         rdfs:label "Data Collection Procedure" .


###  https://w3id.org/rains#DataCollectionSoftware
:DataCollectionSoftware rdf:type owl:Class ;
                        rdfs:subClassOf <http://ontosoft.org/software#Software> ;
                        rdfs:comment "A sao:InformationElement (which is also an osw:Software and a prov:SoftwareAgent)  which records  information detailing  a software that was used to collect data that formed the dataset which was used to train the ML Model" ;
                        rdfs:label "Data Collection Software" .


###  https://w3id.org/rains#DataPreprocessingProcedure
:DataPreprocessingProcedure rdf:type owl:Class ;
                            rdfs:subClassOf sao:InformationElement ;
                            rdfs:comment "A specific type of sao:InformationElement which records a specific piece of  information detailing how the data is preprocessed. This includes information on what is done to the data (e.g., cleaning, sampling, filtering, feature engineering, etc) and how it is done (e.g., manually, programmatically, or using feature engineering software)." ;
                            rdfs:label "Data Preprocessing Procedure" .


###  https://w3id.org/rains#DataPreprocessingSoftware
:DataPreprocessingSoftware rdf:type owl:Class ;
                           rdfs:subClassOf <http://ontosoft.org/software#Software> ;
                           rdfs:comment "A sao:InformationElement (which is also an osw:Software and a prov:SoftwareAgent)  which records  information detailing  a software  that was used to preprocess the dataset using to train the ML Model" ;
                           rdfs:label "Data Preprocessing Software" .


###  https://w3id.org/rains#DatasetComponent
:DatasetComponent rdf:type owl:Class ;
                  rdfs:subClassOf sao:AccountableResult ;
                  rdfs:comment "A specific type of an Implementation  which represents the description of an implemented dataset, including its characteristics, how it was collected, its risks, etc. The dataset component represents a collection of collected and pre-processed data. Once it is split, the subclasses TestingDataset, ValidationDataset, and EvaluationDataset can be used." ;
                  rdfs:label "Dataset Component" .


###  https://w3id.org/rains#DatasetSpecification
:DatasetSpecification rdf:type owl:Class ;
                      rdfs:subClassOf :DesignSpecification ;
                      rdfs:comment "A specific type of design specification  which represents the description of planned requirements, characteristics, risks, etc. related to a dataset (e.g., a training dataset for ML model) expected to be used as part of the AI system life cycle." ;
                      rdfs:label "Dataset Specification" .


###  https://w3id.org/rains#Decide
:Decide rdf:type owl:Class ;
        rdfs:subClassOf sao:AccountableAction ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty ep-plan:hasOutputVariable ;
                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onClass :HumanDecision
                        ] ;
        owl:disjointWith :ProduceSpecification ;
        rdfs:comment "A planned step describing a decision to be taken by a human when incorporating accountability in AI Systems, e.g. approve the design of an ML Model." ;
        rdfs:label "Decide" .


###  https://w3id.org/rains#DesignSpecification
:DesignSpecification rdf:type owl:Class ;
                     rdfs:subClassOf sao:AccountableResult ;
                     rdfs:comment "A specific type of planned sao:AccountableResult which represents a high reference to a specification  (e.g. a specification report) that is expected to be produced during the design stage in order to inform the future AI system lifecycle stages (e.g. implementation)." ;
                     rdfs:label "Design Specification" .


###  https://w3id.org/rains#DesignStageAccountabilityPlan
:DesignStageAccountabilityPlan rdf:type owl:Class ;
                               rdfs:subClassOf sao:AccountabilityPlan ;
                               rdfs:comment "A plan that identifies sao:AccountableAction(s) and sao:AccountableResult(s) relevant to the design stage of an AI system and  which are required to be documented for accountability purposes." ;
                               rdfs:label "Design Stage Accountability Plan" .


###  https://w3id.org/rains#Evaluate
:Evaluate rdf:type owl:Class ;
          rdfs:subClassOf sao:AccountableAction ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty ep-plan:hasInputVariable ;
                            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass :EvaluationSpecification
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty ep-plan:hasOutputVariable ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass :Evaluation
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty ep-plan:hasOutputVariable ;
                            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass :ChangeLog
                          ] ;
          rdfs:comment "A planned step describing an evaluation, i.e, testing the system and producing evaluation metrics, that has some impact on the AI System, e.g. the evaluation of the ML model using a specified Dataset." ;
          rdfs:label "Evaluate" .


###  https://w3id.org/rains#Evaluation
:Evaluation rdf:type owl:Class ;
            rdfs:subClassOf sao:AccountableResult ;
            rdfs:comment "A specific type of planned sao:AccountableResult which represents a high reference to testing at least one component and recording the results of the test(s) (e.g. testing the model with a specified dataset) that is expected to be produced during the implementation stage." ;
            rdfs:label "Evaluation" .


###  https://w3id.org/rains#EvaluationDataset
:EvaluationDataset rdf:type owl:Class ;
                   rdfs:subClassOf :DatasetComponent ;
                   rdfs:comment "A specific type of an Dataset Component  which represents a subset of the whole collected and pre-processed datasets after they were merged then split to be fed into the ML Model. The specific type represents the data used to evaluate the ML Model. The other subsets are used to train and evaluate the model. This type can also represent a standalone dataset used to evaluate the model independed of the datasets used to train and validate it." ;
                   rdfs:label "Evaluation Dataset" .


###  https://w3id.org/rains#EvaluationMeassure
:EvaluationMeassure rdf:type owl:Class ;
                    rdfs:subClassOf sao:InformationElement ;
                    rdfs:comment "A  sao:InformationElement which records a specific piece of  information detailing some measure used as part of an evaluation process. This is broader than mls:EvaluationMeassure which describes a measure related to a model performance." ;
                    rdfs:label "Evaluation Measure" .


###  https://w3id.org/rains#EvaluationProcedure
:EvaluationProcedure rdf:type owl:Class ;
                     rdfs:subClassOf sao:InformationElement ;
                     rdfs:comment "A sao:InformationElement  which records a specific piece of  information detailing procedure by which some aspects of an :AI_Sytem were evaluated. this is broader than mls:EvaluationProcedure which relates to model evaluation." ;
                     rdfs:label "Evaluation Procedure" .


###  https://w3id.org/rains#EvaluationResult
:EvaluationResult rdf:type owl:Class ;
                  rdfs:subClassOf sao:InformationElement ;
                  rdfs:comment "A  sao:InformationElement which records a specific piece of  information detailing the result of an evaluation process. This is broader than mls:EvaluationMeassure which describes a result related to a specific model evaluation." ;
                  rdfs:label "Evaluation Result" .


###  https://w3id.org/rains#EvaluationSpecification
:EvaluationSpecification rdf:type owl:Class ;
                         rdfs:subClassOf :DesignSpecification ;
                         rdfs:comment "A specific type of design specification which represents the description  of a planned evaluation process." ;
                         rdfs:label "Evaluation Specification" .


###  https://w3id.org/rains#ExcludedObject
:ExcludedObject rdf:type owl:Class ;
                rdfs:subClassOf sao:InformationElement ;
                rdfs:comment "A sao:InformationElement  which records  information detailing  a piece of data, software or hardware that should have been realized during the Implementation stage, if the design specification was followed, but was not." ;
                rdfs:label "Excluded Object" .


###  https://w3id.org/rains#FairnessOrBiasDetectionSoftware
:FairnessOrBiasDetectionSoftware rdf:type owl:Class ;
                                 rdfs:subClassOf <http://ontosoft.org/software#Software> ;
                                 rdfs:comment "A sao:InformationElement (which is also an osw:Software and a prov:SoftwareAgent)  which records  information detailing  a software that was used to detect bias or check for fairness in both the dataset which was used to train the ML Model as well as the ML model itself" ;
                                 rdfs:label "Fairness Or Bias Detection Software" .


###  https://w3id.org/rains#FitnessConfirmationDecision
:FitnessConfirmationDecision rdf:type owl:Class ;
                             rdfs:subClassOf :HumanDecision ;
                             rdfs:comment "A specific type of planned human decision representing confirmation of fitness of some sao:AccountableResult t to its purpose." ;
                             rdfs:label "Fitness Confirmation Decision" .


###  https://w3id.org/rains#Funding
:Funding rdf:type owl:Class ;
         rdfs:subClassOf sao:InformationElement ;
         rdfs:comment "A sao:InformationElement  which records a specific piece of  information detailing a funding source." ;
         rdfs:label "Funding" .


###  https://w3id.org/rains#GenerateGuideline
:GenerateGuideline rdf:type owl:Class ;
                   rdfs:subClassOf sao:AccountableAction ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty ep-plan:hasOutputVariable ;
                                     owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                     owl:onClass :Guideline
                                   ] ;
                   rdfs:comment "A planned step describing the creation of a guideline that has some impact on the AI System, e.g. the production of the deployment guideline describing how the AI System is to be deployed on-site." ;
                   rdfs:label "Generate Guideline" .


###  https://w3id.org/rains#GrantCertification
:GrantCertification rdf:type owl:Class ;
                    rdfs:subClassOf :Decide ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty ep-plan:hasOutputVariable ;
                                      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                      owl:onClass :CertificationDecision
                                    ] ;
                    rdfs:comment "A planned step describing a certification granting decision to be taken by a human when incorporating accountability in AI Systems, e.g. grant an ISO certificate." ;
                    rdfs:label "Grant Certification" .


###  https://w3id.org/rains#Guideline
:Guideline rdf:type owl:Class ;
           rdfs:subClassOf sao:AccountableResult ;
           rdfs:comment "A specific type of planned sao:AccountableResult which represents a high reference to a guideline  (e.g. a guideline document) that is expected to be produced during the implementation or deployment stages in order to inform the future AI system lifecycle stages (e.g. deployment or muse and maintenance)." ;
           rdfs:label "Guideline" .


###  https://w3id.org/rains#HardLaw
:HardLaw rdf:type owl:Class ;
         rdfs:subClassOf :ComplianceEntity ;
         owl:disjointWith :SoftLaw ;
         rdfs:comment "A specific type of compliance entity which  records a specific piece of  information about the laws and regulations that must be complied with as part of an information realization collection." ;
         rdfs:label "Hard Law" .


###  https://w3id.org/rains#HumanDecision
:HumanDecision rdf:type owl:Class ;
               rdfs:subClassOf sao:AccountableResult ;
               rdfs:comment "A specific type of a planned sao:AccountableResult which represents a decision made by a human." ;
               rdfs:label "Human Decision" .


###  https://w3id.org/rains#Implement
:Implement rdf:type owl:Class ;
           rdfs:subClassOf sao:AccountableAction ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty ep-plan:hasInputVariable ;
                             owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                             owl:onClass :DesignSpecification
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty ep-plan:hasOutputVariable ;
                             owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                             owl:onClass :ChangeLog
                           ] ;
           rdfs:comment "A planned step describing an implementation, i.e, production of a component of the system, that has some impact on the AI System, e.g. the implementation of the ML model." ;
           rdfs:label "Implement" .


###  https://w3id.org/rains#ImplementationStageAccountabilityPlan
:ImplementationStageAccountabilityPlan rdf:type owl:Class ;
                                       rdfs:subClassOf sao:AccountabilityPlan ;
                                       rdfs:comment "A plan that identifies sao:AccountableAction(s) and sao:AccountableResult(s) relevant to the implementation stage of an AI system and  which are required to be documented for accountability purposes." ;
                                       rdfs:label "Implementation Stage Accountability Plan" .


###  https://w3id.org/rains#ImplementedObject
:ImplementedObject rdf:type owl:Class ;
                   rdfs:subClassOf sao:InformationElement ;
                   rdfs:comment "A sao:InformationElement  which records  information detailing  a piece of data, software or hardware that was realized during the Implementation stage following the design specifications." ;
                   rdfs:label "Implemented Object" .


###  https://w3id.org/rains#IncorrectUseCase
:IncorrectUseCase rdf:type owl:Class ;
                  rdfs:subClassOf :UseCase ;
                  owl:disjointWith :IntendedUseCase ;
                  rdfs:comment "A type of :UseCase detailing a situation in which any component of :AI_System or the system as whole should not be used." ;
                  rdfs:label "Incorrect Use Case" .


###  https://w3id.org/rains#IntendedUseCase
:IntendedUseCase rdf:type owl:Class ;
                 rdfs:subClassOf :UseCase ;
                 rdfs:comment "A type of :UseCase detailing a situation in which any component of :AI_System or the system as whole is expected to be used." ;
                 rdfs:label "Intended Use Case" .


###  https://w3id.org/rains#IntendedUserGroup
:IntendedUserGroup rdf:type owl:Class ;
                   rdfs:subClassOf sao:InformationElement ;
                   rdfs:comment "A sao:InformationElement  which records a specific piece of  information detailing the target user audience for the functionality of the overall :AI_System or the components used to develop such system (e.g., a reused public training dataset or a model would be associated with this information)." ;
                   rdfs:label "Intended User Group" .


###  https://w3id.org/rains#Limitation
:Limitation rdf:type owl:Class ;
            rdfs:subClassOf sao:InformationElement ;
            rdfs:comment "A sao:InformationElement  which records a specific piece of  information detailing the limitations of any aspect related to an :AI_System including the system as whole, its components (e.g., ML model),  or associated processes (e.g., evaluation)." ;
            rdfs:label "Limitation" .


